rules_version = '2';
service cloud.firestore {
  match /databases/{database}/documents {
    match /app_config/{docId} {
      allow read: if true;
      allow write: if false;
    }
    match /users/{uid}/items/{itemId} {
      allow read, write: if request.auth != null && request.auth.uid == uid;
    }
  }
}
